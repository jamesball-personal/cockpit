  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "codegen": "rm -rf src/types && mkdir -p src/types && apollo client:codegen --target typescript --localSchemaFile=src/schema.json --outputFlat src/types && rm src/types/globalTypes.ts",
    "download": "rm -f src/schema.json && apollo schema:download --endpoint http://localhost:8080/graphql src/schema.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
