    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "apollo": "^2.27.0",
    "apollo-boost": "^0.4.7",
